    <style>
        [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x- ng-cloak {
            display: none !important;
        }

        .apps-store a img {
            box-shadow: none !important
        }

        .fa-phone {
            -webkit-transform: scaleX(-1);
            transform: scaleX(-1);
        }
